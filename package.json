{
  "name": "gh-pages-demo",
  "homepage": "https://saske-pudjiha.github.io/gh-pages-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "json-server --watch ./src/server/db/index.js --routes ./src/server/routes.json --port 3001",
    "build": "react-scripts build && npm run save-json-api && shx cp build/index.html build/404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "save-json-api": "node ./src/server/scripts/save-json-api.js",
    "predeploy": "rimraf build && npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "gh-pages": "^6.3.0",
    "json-server": "^0.17.4",
    "node-fs": "^0.1.7",
    "rimraf": "^6.1.2",
    "shx": "^0.4.0"
  }
}
